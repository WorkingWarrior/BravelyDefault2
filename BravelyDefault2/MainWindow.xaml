<Window x:Class="BravelyDefault2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BravelyDefault2" xmlns:controls="clr-namespace:BravelyDefault2.Controls"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Bravely Default 2 Save Editor (Nintendo Switch)" Width="1024" Height="580">
    <Window.DataContext>
        <local:ViewModel/>
    </Window.DataContext>
    <Window.Resources>
        <controls:JobToTextConverter x:Key="JobToTextConverter" />
        <controls:TimeSpanToTextConverter x:Key="TimeSpanToTextConverter" />
        <controls:DateTimeToTextConverter x:Key="DateTimeToTextConverter" />
    </Window.Resources>
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="File">
                <MenuItem Header="Open..." Click="MenuItemFileOpen_Click"/>
                <MenuItem Header="Save" Click="MenuItemFileSave_Click"/>
                <MenuItem Header="SaveAs..." Click="MenuItemFileSaveAs_Click"/>
                <Separator/>
                <MenuItem Header="Import..." Click="MenuItemFileImport_Click"/>
                <MenuItem Header="Export..." Click="MenuItemFileExport_Click"/>
                <MenuItem Header="Exit" Click="MenuItemFileExit_Click"/>
            </MenuItem>
        </Menu>
        <TabControl>
            <TabItem Header="Basic" IsEnabled="{Binding SaveLoaded}">
                <Grid IsEnabled="{Binding SaveLoaded}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Label Grid.Column="0" Grid.Row="0" Content="Money" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="1" Grid.Row="0" Text="{Binding Money, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Center"/>
                    <Label Grid.Column="0" Grid.Row="1" Content="Save time" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="1" Grid.Row="1" Text="{Binding SaveDate, Converter={StaticResource DateTimeToTextConverter}}" VerticalAlignment="Center" IsReadOnly="True"/>
                    <Label Grid.Column="0" Grid.Row="2" Content="Play time" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="1" Grid.Row="2" Text="{Binding PlayTime, Converter={StaticResource TimeSpanToTextConverter}}" VerticalAlignment="Center" IsReadOnly="True"/>
                </Grid>
            </TabItem>
            <TabItem Header="Seth" IsEnabled="{Binding SaveLoaded}">
