                            <TextBox x:Name="TextBox_Seth_HP" Grid.Column="1" Text="{Binding ViewModelSeth.HP, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" VerticalAlignment="Center" PreviewTextInput="NumberValidationTextBox"/>
